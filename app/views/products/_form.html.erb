<%= form_for @product do |f| %>
  <%= f.hidden_field :vendor_id%>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>

  <%= f.label :price %><br>
  <%= f.text_field :price %><br>

  <%= f.label :category %><br>
  <%= f.text_field :category %><br>

  <%= f.label :description %><br>
  <%= f.text_area :description %><br>
  
  <%= f.submit %>
<% end %>