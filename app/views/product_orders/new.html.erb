<%= form_for @product_order, url: {action: "create"} do |f| %>
  <%#byebug%>
  <% f.fields_for :order do |order_field|%>
  <%= order_field.label :delivery_date %><br>
  <%= order_field.date_field :delivery_date %><br>
  
  
  <%= f.label "Add product to your order"%><br>
  <%= f.collection_select  :product_id, Product.all, :id, :name%>
  <%= f.label :quantity%>
  <%= f.number_field :quantity%>
  <br>
  <%= order_field.label :delivered %><br>
  <%= order_field.check_box :delivered %><br>
   <% end %>   
  <%= f.submit "Place your order"%>
<% end %>
  